  "bugs": {
    "url": "https://github.com/wyoung60/freshnessTracker/issues"
  },
  "homepage": "https://github.com/wyoung60/freshnessTracker#readme",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.9"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "pg": "^8.6.0",
    "sequelize": "^6.6.4"
  }
}
